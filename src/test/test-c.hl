greeting = "Howdy";
name = "Partner";
HOLLER greeting " " name;

IF_YALL (greeting == "Howdy") {
  HOLLER "Yeehaw!";
}

x = "global";

IF_YALL (x == "global") {
    x = "modified";
    y = "new";
}

HOLLER x;
HOLLER y;

WHILE_YALL (name == "Partner") {
  HOLLER "Still riding...";
  name = "Cowboy";
}